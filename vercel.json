{
  "version": 2,
  "buildCommand": "npm run build:skip-fetch",
  "outputDirectory": "dist/public",
  "framework": null,
  "functions": {
    "api/**/*.ts": {
      "includeFiles": "parsed_data/**"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
